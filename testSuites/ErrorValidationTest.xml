
<suite parallel = "tests" name="ErrorValidation">
    <listeners>
        <listener class-name = "SeleniumFrameworkPractice.TestComponents.Listeners"/>
    </listeners>
	<groups>
		<run>
			<include name = "ErrorHandling"/>
		</run>
		</groups>
  <test thread-count="5" name="Submit Order Test">
    <classes>
      <class name="SeleniumFrameworkPractice.Tests.SubmitOrderTest"/>
    </classes>
  </test> <!-- Test -->
  <test thread-count="5" name="Error Validations Test">
    <classes>
      <class name="SeleniumFrameworkPractice.Tests.ErrorValidation"/>
    </classes>
  </test> <!-- Test -->

</suite> <!-- Suite -->

      
 
 